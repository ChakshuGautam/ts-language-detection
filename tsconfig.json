{
    "compilerOptions": {
        "noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
        "target": "es5",
        "types": [
            "node"
        ],
        "outDir": "build/ts"
    },
    "include": [
        "src/**.ts"
    ]
}